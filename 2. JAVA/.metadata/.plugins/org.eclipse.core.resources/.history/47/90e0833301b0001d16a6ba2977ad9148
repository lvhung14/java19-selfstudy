public class Ex01 {
	public static void main(String[] args) {
		Scanner hi = new Scanner(System.in);
		Boolean A = Character.isDigit(5);
		System.out.println(A);
	}
	
boolean matches() {};
	public static boolean multiples(int pCheck) {
		if(pCheck % 2 == 0) {
			return true;
		}
		else return false;
	}
	
	public static void check(int pDigit) {
		
		Boolean A = Character.isDigit(pDigit);
		System.out.println(A);
	}
}
